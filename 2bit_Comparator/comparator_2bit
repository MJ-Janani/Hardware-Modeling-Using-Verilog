S_0x559944a5f850 .scope module, "comparator_2bit_tb" "comparator_2bit_tb" 2 1;
 .timescale 0 0;
v0x559944a75e10_0 .var "a0", 0 0;
v0x559944a75ed0_0 .var "a1", 0 0;
v0x559944a75f70_0 .var "b0", 0 0;
v0x559944a76010_0 .var "b1", 0 0;
v0x559944a760e0_0 .net "eq", 0 0, v0x559944a75aa0_0;  1 drivers
