    %store/vec4 v0x559944a76010_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559944a75e10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a75ed0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559944a75f70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559944a76010_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a75e10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a75ed0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a75f70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a76010_0, 0, 1;
