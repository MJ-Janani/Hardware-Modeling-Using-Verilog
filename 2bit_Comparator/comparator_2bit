v0x559944a762a0_0 .net "lt", 0 0, v0x559944a75c70_0;  1 drivers
S_0x559944a5f9d0 .scope module, "uut" "comparator_2bit" 2 5, 3 1 0, S_0x559944a5f850;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a0"
    .port_info 1 /INPUT 1 "a1"
    .port_info 2 /INPUT 1 "b0"
    .port_info 3 /INPUT 1 "b1"
    .port_info 4 /OUTPUT 1 "gt"
    .port_info 5 /OUTPUT 1 "lt"
    .port_info 6 /OUTPUT 1 "eq"
v0x559944a3b440_0 .net "a0", 0 0, v0x559944a75e10_0;  1 drivers
v0x559944a75880_0 .net "a1", 0 0, v0x559944a75ed0_0;  1 drivers
v0x559944a75940_0 .net "b0", 0 0, v0x559944a75f70_0;  1 drivers
v0x559944a759e0_0 .net "b1", 0 0, v0x559944a76010_0;  1 drivers
v0x559944a75aa0_0 .var "eq", 0 0;
v0x559944a75bb0_0 .var "gt", 0 0;
v0x559944a75c70_0 .var "lt", 0 0;
E_0x559944a5dc80/0 .event edge, v0x559944a75880_0, v0x559944a3b440_0, v0x559944a759e0_0, v0x559944a75940_0;
E_0x559944a5dc80/1 .event edge, v0x559944a75bb0_0, v0x559944a75c70_0, v0x559944a75aa0_0;
E_0x559944a5dc80 .event/or E_0x559944a5dc80/0, E_0x559944a5dc80/1;
    .scope S_0x559944a5f9d0;
T_0 ;
    %wait E_0x559944a5dc80;
    %load/vec4 v0x559944a759e0_0;
    %load/vec4 v0x559944a75940_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x559944a75880_0;
    %load/vec4 v0x559944a3b440_0;
    %concat/vec4; draw_concat_vec4
    %cmp/u;
    %flag_get/vec4 5;
    %store/vec4 v0x559944a75bb0_0, 0, 1;
    %load/vec4 v0x559944a75880_0;
    %load/vec4 v0x559944a3b440_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x559944a759e0_0;
    %load/vec4 v0x559944a75940_0;
    %concat/vec4; draw_concat_vec4
    %cmp/u;
    %flag_get/vec4 5;
    %store/vec4 v0x559944a75c70_0, 0, 1;
    %load/vec4 v0x559944a75880_0;
    %load/vec4 v0x559944a3b440_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x559944a759e0_0;
    %load/vec4 v0x559944a75940_0;
    %concat/vec4; draw_concat_vec4
    %cmp/e;
    %flag_get/vec4 4;
    %store/vec4 v0x559944a75aa0_0, 0, 1;
    %vpi_call 3 9 "$display", "{a1,a0}={%b,%b} when compared with {b1,b0}={%b,%b} gives gt=%b,lt=%b,eq=%b", v0x559944a75880_0, v0x559944a3b440_0, v0x559944a759e0_0, v0x559944a759e0_0, v0x559944a75940_0, v0x559944a75bb0_0, v0x559944a75c70_0, v0x559944a75aa0_0 {0 0 0};
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x559944a5f850;
T_1 ;
    %vpi_call 2 8 "$monitor", "Time=%2d : a={%b,%b}, b={%b,%b} => gt=%b, lt=%b, eq=%b", $time, v0x559944a75ed0_0, v0x559944a75e10_0, v0x559944a76010_0, v0x559944a75f70_0, v0x559944a761d0_0, v0x559944a762a0_0, v0x559944a760e0_0 {0 0 0};
    %vpi_call 2 9 "$dumpfile", "comparator_2bit.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x559944a5f850 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x559944a75e10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a75ed0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x559944a75f70_0, 0, 1;
